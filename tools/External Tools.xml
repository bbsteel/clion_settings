<toolSet name="External Tools">
  <tool name="clean_dump" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/bin/bash" />
      <option name="PARAMETERS" value="-c &quot;mkdir -p /tmp/dump; rm  -rf  /tmp/dump/*&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>